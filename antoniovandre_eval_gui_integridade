#!/bin/bash

# Projeto Mathematical Ramblings.

# Script verificador de integridade de antoniovandre_eval_gui, versão de distribuição.

# Licença: Atribuição-NãoComercial-CompartilhaIgual (CC BY-NC-SA).

# Última atualização: 20-03-2025. Não considerando alterações em variáveis globais.

TRADUCOESARQUIVO="${1}"
MD5SUMTRADUCOESARQUIVO="961cedecc3f7f25515f009343ba34fe5"

HASHS="20250320"

if ! ls hashfile &>> /dev/null; then exit 1; fi

while read linha; do if [ "${linha}" != "" ] && [ "$(echo -n ${linha} | awk '{print $1}')" != "#" ]; then HASH=$linha; break; fi; done < hashfile

FLAG="0"

OLDIFS=$IFS; IFS=","; for hash in `echo -n "${HASHS}"`; do if [ "${hash}" == "${HASH}" ]; then FLAG="1"; fi; done; IFS=$OLDIFS

if [ "${FLAG}" == "0" ]; then exit 1; fi

if [ "${HASH}" == "20250320" ]; then if ! ls "${TRADUCOESARQUIVO}" &>> /dev/null || ! ls index.html &>> /dev/null || ! ls index_en.html &>> /dev/null || ! ls index_cn.html &>> /dev/null || ! ls index_es.html &>> /dev/null || ! ls index_in.html &>> /dev/null || ! ls index_ar.html &>> /dev/null || ! ls index_ru.html &>> /dev/null || ! ls README.txt &>> /dev/null || ! ls README_en.txt &>> /dev/null || ! ls README_cn.txt &>> /dev/null || ! ls README_es.txt &>> /dev/null || ! ls README_in.txt &>> /dev/null || ! ls README_ar.txt &>> /dev/null || ! ls README_ru.txt &>> /dev/null || ! ls README.md &>> /dev/null || ! ls antoniovandre_evalGUI.c &>> /dev/null || ! ls antoniovandre_constantes.c &>> /dev/null || ! ls antoniovandre_eval_gui.pytemp &>> /dev/null || ! ls antoniovandre_cores &>> /dev/null || ! ls antoniovandre_sobre.txt &>> /dev/null || ! ls antoniovandre_sobre_en.txt &>> /dev/null || ! ls antoniovandre_sobre_cn.txt &>> /dev/null || ! ls antoniovandre_sobre_es.txt &>> /dev/null || ! ls antoniovandre_sobre_in.txt &>> /dev/null || ! ls antoniovandre_sobre_ar.txt &>> /dev/null || ! ls antoniovandre_sobre_ru.txt &>> /dev/null || ! ls antoniovandre_precisao_real.txt &>> /dev/null || ! ls Math/antoniovandre_eval.c &>> /dev/null || ! ls antoniovandre_eval_gui_bgimage.png &>> /dev/null || ! ls antoniovandre_eval_gui_logo.png &>> /dev/null || ! ls antoniovandre_eval_gui_logo_200p.png &>> /dev/null || ! ls "Bandeira do Brasil - 320p.png" &>> /dev/null || ! ls "Bandeira do Reino Unido - 510p.png" &>> /dev/null || ! ls "Bandeira da China - 320p.png" &>> /dev/null || ! ls "Bandeira da Espanha - 320p.png" &>> /dev/null || ! ls "Bandeira da Índia - 320p.png" &>> /dev/null || ! ls "Bandeira da Arábia Saudita - 320p.png" &>> /dev/null || ! ls "Bandeira da Rússia - 320p.png" &>> /dev/null || ! ls DropBox-Icon.png &>> /dev/null || ! ls "GitHub logo.png" &>> /dev/null || ! ls "Mathematical Ramblings - Smartphone wallpaper 3 - Indexada.png" &>> /dev/null || ! ls by-nc-sa.jpg &>> /dev/null || [ "$(md5sum ${TRADUCOESARQUIVO} 2>> /dev/null | cut -d ' ' -f 1)" != "${MD5SUMTRADUCOESARQUIVO}" ] || [ "$(md5sum index.html 2>> /dev/null | cut -d ' ' -f 1)" != "7d4724216441720f96fcb2f512a5aa48" ] || [ "$(md5sum index_en.html 2>> /dev/null | cut -d ' ' -f 1)" != "c8b12691303e2db2571231e532004e4f" ] || [ "$(md5sum index_cn.html 2>> /dev/null | cut -d ' ' -f 1)" != "a19c8ea251c3d35f6ef49de4ff5de58f" ] || [ "$(md5sum index_es.html 2>> /dev/null | cut -d ' ' -f 1)" != "531f8596e03e080727b02d6bc6d0b46f" ] || [ "$(md5sum index_in.html 2>> /dev/null | cut -d ' ' -f 1)" != "f6b96ba3e11387d6e3eec5ce7f15ddb5" ] || [ "$(md5sum index_ar.html 2>> /dev/null | cut -d ' ' -f 1)" != "9d280e6c53a1c1c1758393ceff7d0715" ] || [ "$(md5sum index_ru.html 2>> /dev/null | cut -d ' ' -f 1)" != "d2578a6583ff3ea58cd21c05a3a820b1" ] || [ "$(md5sum README.txt 2>> /dev/null | cut -d ' ' -f 1)" != "6db6c143fc0e8d1692213533c2e601bd" ] || [ "$(md5sum README_en.txt 2>> /dev/null | cut -d ' ' -f 1)" != "c333a9c23d0690cceb0b64779a5093fa" ] || [ "$(md5sum README_cn.txt 2>> /dev/null | cut -d ' ' -f 1)" != "73463fd9302e5b7327bbb3a5fa171b88" ] || [ "$(md5sum README_es.txt 2>> /dev/null | cut -d ' ' -f 1)" != "975db9046137955b1df6de7e48cb612f" ] || [ "$(md5sum README_in.txt 2>> /dev/null | cut -d ' ' -f 1)" != "73ede574e68e4cc02ecd9ceb3d91c0f7" ] || [ "$(md5sum README_ar.txt 2>> /dev/null | cut -d ' ' -f 1)" != "d16fd9a884ccdb515bbf9a90141712ad" ] || [ "$(md5sum README_ru.txt 2>> /dev/null | cut -d ' ' -f 1)" != "e49295d1c457fdc9beea03136b723f47" ] || [ "$(md5sum README.md 2>> /dev/null | cut -d ' ' -f 1)" != "21a1a42bba35c4206728c695815d335a" ] || [ "$(md5sum antoniovandre_evalGUI.c 2>> /dev/null | cut -d ' ' -f 1)" != "346c957a096e9aff42b06865d73a9e83" ] || [ "$(md5sum antoniovandre_constantes.c 2>> /dev/null | cut -d ' ' -f 1)" != "38c989b6aeb167d53201107a0360a119" ] || [ "$(md5sum antoniovandre_eval_gui.pytemp 2>> /dev/null | cut -d ' ' -f 1)" != "9bdccc902177d8b6f8e1c82cae2c4e64" ] || [ "$(md5s