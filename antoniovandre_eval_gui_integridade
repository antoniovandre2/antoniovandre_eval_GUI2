] || [ "$(md5sum 'Bandeira da Índia - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "11543ddc6c7e1721aed4a3cf67c25267" ] || [ "$(md5sum 'Bandeira da Arábia Saudita - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "e460801eac7ce12c1eb996dab41ee4df" ] || [ "$(md5sum 'Bandeira da Rússia - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "58f050aef6773d8037164f2a28e3fb8b" ] || [ "$(md5sum DropBox-Icon.png 2>> /dev/null | cut -d ' ' -f 1)" != "96a57837a41cd9250459104b46d2f64e" ] || [ "$(md5sum 'GitHub logo.png' 2>> /dev/null | cut -d ' ' -f 1)" != "482dce8fe2ffc91fd287d8118f3488c2" ] || [ "$(md5sum "Mathematical Ramblings - Smartphone wallpaper 3 - Indexada.png" 2>> /dev/null | cut -d ' ' -f 1)" != "09e1440f8222651114f6f60d16889360" ] || [ "$(md5sum by-nc-sa.jpg 2>> /dev/null | cut -d ' ' -f 1)" != "e1746d3e902bc8e309335b983cd0bc00" ]; then exit 1; fi; fi
