#!/bin/bash

# Projeto Mathematical Ramblings (mathematicalramblings.blogspot.com).

# Script para compilar e instalar antoniovandre_eval_gui, versão de distribuição.

# Licença: Atribuição-NãoComercial-CompartilhaIgual (CC BY-NC-SA).

# Última atualização: 01-02-2023.

SOURCEDIR="Math/"
DESTDIR="/usr/bin/"
DESTDIRSHARE="/usr/share/"

TRADUCOESARQUIVO="antoniovandre_traducoes.txt"
MD5SUMTRADUCOESARQUIVO="ea4c7b95b5992d5f3adb3506a6479dec"

TIMEOUTMENSAGEM="6"

if [ "${1}" == "instalar" ]; then if ! ls "${TRADUCOESARQUIVO}" &>> /dev/null || ! ls index.html &>> /dev/null || ! ls index_en.html &>> /dev/null || ! ls index_cn.html &>> /dev/null || ! ls index_es.html &>> /dev/null || ! ls index_in.html &>> /dev/null || ! ls index_ar.html &>> /dev/null || ! ls index_ru.html &>> /dev/null || ! ls README.txt &>> /dev/null || ! ls README_en.txt &>> /dev/null || ! ls README_cn.txt &>> /dev/null || ! ls README_es.txt &>> /dev/null || ! ls README_in.txt &>> /dev/null || ! ls README_ar.txt &>> /dev/null || ! ls README_ru.txt &>> /dev/null || ! ls README.md &>> /dev/null || ! ls antoniovandre.c &>> /dev/null || ! ls antoniovandre_constantes.c &>> /dev/null || ! ls antoniovandre_eval_gui.pytemp &>> /dev/null || ! ls antoniovandre_cores &>> /dev/null || ! ls antoniovandre_sobre.txt &>> /dev/null || ! ls antoniovandre_sobre_en.txt &>> /dev/null || ! ls antoniovandre_sobre_cn.txt &>> /dev/null || ! ls antoniovandre_sobre_es.txt &>> /dev/null || ! ls antoniovandre_sobre_in.txt &>> /dev/null || ! ls antoniovandre_sobre_ar.txt &>> /dev/null || ! ls antoniovandre_sobre_ru.txt &>> /dev/null || ! ls antoniovandre_precisao_real.txt &>> /dev/null || ! ls Math/antoniovandre_eval.c &>> /dev/null || ! ls antoniovandre_eval_gui_bgimage.png &>> /dev/null || ! ls antoniovandre_eval_gui_logo.png &>> /dev/null || ! ls antoniovandre_eval_gui_logo_200p.png &>> /dev/null || ! ls "Bandeira do Brasil - 320p.png" &>> /dev/null || ! ls "Bandeira do Reino Unido - 510p.png" &>> /dev/null || ! ls "Bandeira da China - 320p.png" &>> /dev/null || ! ls "Bandeira da Espanha - 320p.png" &>> /dev/null || ! ls "Bandeira da Índia - 320p.png" &>> /dev/null || ! ls "Bandeira da Arábia Saudita - 320p.png" &>> /dev/null || ! ls "Bandeira da Rússia - 320p.png" &>> /dev/null || ! ls DropBox-Icon.png &>> /dev/null || ! ls "GitHub logo.png" &>> /dev/null || ! ls "Mathematical Ramblings - Smartphone wallpaper 3 - Indexada.png" &>> /dev/null || ! ls by-nc-sa.jpg &>> /dev/null || [ "$(md5sum ${TRADUCOESARQUIVO} 2>> /dev/null | cut -d ' ' -f 1)" != "${MD5SUMTRADUCOESARQUIVO}" ] || [ "$(md5sum index.html 2>> /dev/null | cut -d ' ' -f 1)" != "e37c5699a39b48442d1162ff9007b938" ] || [ "$(md5sum index_en.html 2>> /dev/null | cut -d ' ' -f 1)" != "9c440f0e739720d1d0378989b64b070f" ] || [ "$(md5sum index_cn.html 2>> /dev/null | cut -d ' ' -f 1)" != "10b772e0f76b86625db1ecfb1daddbba" ] || [ "$(md5sum index_es.html 2>> /dev/null | cut -d ' ' -f 1)" != "fb1d412045773b284a666585a7d2024f" ] || [ "$(md5sum index_in.html 2>> /dev/null | cut -d ' ' -f 1)" != "1ac5318adea1c93cee50a06545e16adf" ] || [ "$(md5sum index_ar.html 2>> /dev/null | cut -d ' ' -f 1)" != "8ec8948aed3c42fbaf5211dafd0c6ebc" ] || [ "$(md5sum index_ru.html 2>> /dev/null | cut -d ' ' -f 1)" != "57e05909c87fb00864a3040180d9de17" ] || [ "$(md5sum README.txt 2>> /dev/null | cut -d ' ' -f 1)" != "232ccc035fcfd5237756cf25c057756b" ] || [ "$(md5sum README_en.txt 2>> /dev/null | cut -d ' ' -f 1)" != "50ce868a60d9170791afa9e2c16819cd" ] || [ "$(md5sum README_cn.txt 2>> /dev/null | cut -d ' ' -f 1)" != "ae45536f05615c015534f9c11d5b510a" ] || [ "$(md5sum README_es.txt 2>> /dev/null | cut -d ' ' -f 1)" != "2a63a4b7ed96dcaa0b064f5133e7b560" ] || [ "$(md5sum README_in.txt 2>> /dev/null | cut -d ' ' -f 1)" != "27eb91ca8006aec2df454c4faf85cd43" ] || [ "$(md5sum README_ar.txt 2>> /dev/null | cut -d ' ' -f 1)" != "21dbcc27dd300b363bc05e92fe538bb8" ] || [ "$(md5sum README_ru.txt 2>> /dev/null | cut -d ' ' -f 1)" != "9a880bd54cb5a9ac260dc13697d8f1fa" ] || [ "$(md5sum README.md 2>> /dev/null | cut -d ' ' -f 1)" != "741faabca3bbc07db63efb10a8669924" ] || [ "$(md5sum antoniovandre.c 2>> /dev/null | cut -d ' ' -f 1)" != "9ae0e026b5d93484bc2aea1c8fc68271" ] || [ "$(md5sum antoniovandre_constantes.c 2>> /dev/null | cut -d ' ' -f 1)" != "38c989b6aeb167d53201107a0360a119" ] || [ "$(md5sum antoniovandre_eval_gui.pytemp 2>> /dev/null | cut -d ' ' -f 1)" != "0f7149f58822bfa0f1ecb6ce9055b829" ] || [ "$(md5sum antoniovandre_cores 2>> /dev/null | cut -d ' ' -f 1)" != "d4f1a5742145b4d790698eed8eaf7d50" ] || [ "$(md5sum antoniovandre_sobre.txt 2>> /dev/null | cut -d ' ' -f 1)" != "02716e2596e73cd986458c54bb740be6" ] || [ "$(md5sum antoniovandre_sobre_en.txt 2>> /dev/null | cut -d ' ' -f 1)" != "b1bb3df7c86e527deff06fa337920d7e" ] || [ "$(md5sum antoniovandre_sobre_cn.txt 2>> /dev/null | cut -d ' ' -f 1)" != "68d3c28c7fa52085c116de9547aea34c" ] || [ "$(md5sum antoniovandre_sobre_es.txt 2>> /dev/null | cut -d ' ' -f 1)" != "5179da186137717fa8f157cfa6684d76" ] || [ "$(md5sum antoniovandre_sobre_in.txt 2>> /dev/null | cut -d ' ' -f 1)" != "0002772bbc4126d600a19901f5067da7" ] || [ "$(md5sum antoniovandre_sobre_ar.txt 2>> /dev/null | cut -d ' ' -f 1)" != "8f612ae335ee31cc6848e9f3085367c2" ] || [ "$(md5sum antoniovandre_sobre_ru.txt 2>> /dev/null | cut -d ' ' -f 1)" != "f31aa3d022be9f33f34e7771ab0d1161" ] || [ "$(md5sum antoniovandre_precisao_real.txt 2>> /dev/null | cut -d ' ' -f 1)" != "b7e7cb558e4f35d14cc311a04af6eb1c" ] || [ "$(md5sum Math/antoniovandre_eval.c 2>> /dev/null | cut -d ' ' -f 1)" != "6abc4d95f1798e4c030f8c783e7a3e62" ] || [ "$(md5sum antoniovandre_eval_gui_bgimage.png 2>> /dev/null | cut -d ' ' -f 1)" != "89cb7026063ee5e52291ede3d75d730d" ] || [ "$(md5sum antoniovandre_eval_gui_logo.png 2>> /dev/null | cut -d ' ' -f 1)" != "a7fcaccb1a993d65a644d781b4fa8d59" ] || [ "$(md5sum antoniovandre_eval_gui_logo_200p.png 2>> /dev/null | cut -d ' ' -f 1)" != "77d3ec93b5775b1029c6099ed6421b20" ] || [ "$(md5sum 'Bandeira do Brasil - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "386b8d48330e3a2d38f4b746499a54e4" ] || [ "$(md5sum 'Bandeira do Reino Unido - 510p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "d037587453fb413f1b8b0d7ac4f32861" ] || [ "$(md5sum 'Bandeira da China - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "367062c215b6050b1aa453e02cc44532" ] || [ "$(md5sum 'Bandeira da Espanha - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "b4acbd3849804d8799195ce621c96380" ] || [ "$(md5sum 'Bandeira da Índia - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "11543ddc6c7e1721aed4a3cf67c25267" ] || [ "$(md5sum 'Bandeira da Arábia Saudita - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "e460801eac7ce12c1eb996dab41ee4df" ] || [ "$(md5sum 'Bandeira da Rússia - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "58f050aef6773d8037164f2a28e3fb8b" ] || [ "$(md5sum DropBox-Icon.png 2>> /dev/null | cut -d ' ' -f 1)" != "96a57837a41cd9250459104b46d2f64e" ] || [ "$(md5sum 'GitHub logo.png' 2>> /dev/null | cut -d ' ' -f 1)" != "482dce8fe2ffc91fd287d8118f3488c2" ] || [ "$(md5sum "Mathematical Ramblings - Smartphone wallpaper 3 - Indexada.png" 2>> /dev/null | cut -d ' ' -f 1)" != "09e1440f8222651114f6f60d16889360" ] || [ "$(md5sum by-nc-sa.jpg 2>> /dev/null | cut -d ' ' -f 1)" != "e1746d3e902bc8e309335b983cd0bc00" ]; then echo -e "Diretório corrompido / Corrupted directory / 目錄損壞 / Directorio corrupto / Поврежденный каталог."; exit 1; fi; fi

if [ -n "${2}" ]; then ARGUMENTOLINGUISTICO="${2}"; else ARGUMENTOLINGUISTICO="nulo"; fi

case "${ARGUMENTOLINGUISTICO}" in
	"nulo")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
	;;
	"pt")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
	;;
	"br")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
	;;
	"en")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 2)")
	;;
	"us")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 2)")
	;;
	"cn")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 3)")
	;;
	"es")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 4)")
	;;
	"in")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 5)")
	;;
	"ar")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
	;;
	"ru")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
	;;
	*)
		echo -e "Língua não reconhecida / Unrecognized langage / 無法識別的語言 / Lenguaje no reconocido / अपरिचित भाषा / لغة غير معروفة / Нераспознанный язык."; exit 1
	;;
esac

if ! [ -n "$AVCORES" ]; then echo -ne "${CARREGANDOCORES}"; if ! source "antoniovandre_cores" &>> /dev/null; then echo -e "Ocorreu um erro.\n"; else echo -e "${AV_VERDE}Ok${AV_SEMCOR}.\n"; fi; fi

