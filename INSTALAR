#!/bin/bash

# Projeto Mathematical Ramblings (mathematicalramblings.blogspot.com).

# Script para compilar e instalar antoniovandre_eval_gui, versão de distribuição.

# Licença: Atribuição-NãoComercial-CompartilhaIgual (CC BY-NC-SA).

# Última atualização: 03-08-2024. Não considerando alterações em variáveis globais.

SOURCEDIR="Math/"

DESTDIR="/usr/bin/"
DESTDIRSHARE="/usr/share/"

TRADUCOESARQUIVO="antoniovandre_traducoes.txt"
MD5SUMTRADUCOESARQUIVO="961cedecc3f7f25515f009343ba34fe5"

TIMEOUTMENSAGEM="6"

if [ "${1}" == "instalar" ]; then if ! ls "${TRADUCOESARQUIVO}" &>> /dev/null || ! ls index.html &>> /dev/null || ! ls index_en.html &>> /dev/null || ! ls index_cn.html &>> /dev/null || ! ls index_es.html &>> /dev/null || ! ls index_in.html &>> /dev/null || ! ls index_ar.html &>> /dev/null || ! ls index_ru.html &>> /dev/null || ! ls README.txt &>> /dev/null || ! ls README_en.txt &>> /dev/null || ! ls README_cn.txt &>> /dev/null || ! ls README_es.txt &>> /dev/null || ! ls README_in.txt &>> /dev/null || ! ls README_ar.txt &>> /dev/null || ! ls README_ru.txt &>> /dev/null || ! ls README.md &>> /dev/null || ! ls antoniovandre_evalGUI.c &>> /dev/null || ! ls antoniovandre_constantes.c &>> /dev/null || ! ls antoniovandre_eval_gui.pytemp &>> /dev/null || ! ls antoniovandre_cores &>> /dev/null || ! ls antoniovandre_sobre.txt &>> /dev/null || ! ls antoniovandre_sobre_en.txt &>> /dev/null || ! ls antoniovandre_sobre_cn.txt &>> /dev/null || ! ls antoniovandre_sobre_es.txt &>> /dev/null || ! ls antoniovandre_sobre_in.txt &>> /dev/null || ! ls antoniovandre_sobre_ar.txt &>> /dev/null || ! ls antoniovandre_sobre_ru.txt &>> /dev/null || ! ls antoniovandre_precisao_real.txt &>> /dev/null || ! ls Math/antoniovandre_eval.c &>> /dev/null || ! ls antoniovandre_eval_gui_bgimage.png &>> /dev/null || ! ls antoniovandre_eval_gui_logo.png &>> /dev/null || ! ls antoniovandre_eval_gui_logo_200p.png &>> /dev/null || ! ls "Bandeira do Brasil - 320p.png" &>> /dev/null || ! ls "Bandeira do Reino Unido - 510p.png" &>> /dev/null || ! ls "Bandeira da China - 320p.png" &>> /dev/null || ! ls "Bandeira da Espanha - 320p.png" &>> /dev/null || ! ls "Bandeira da Índia - 320p.png" &>> /dev/null || ! ls "Bandeira da Arábia Saudita - 320p.png" &>> /dev/null || ! ls "Bandeira da Rússia - 320p.png" &>> /dev/null || ! ls DropBox-Icon.png &>> /dev/null || ! ls "GitHub logo.png" &>> /dev/null || ! ls "Mathematical Ramblings - Smartphone wallpaper 3 - Indexada.png" &>> /dev/null || ! ls by-nc-sa.jpg &>> /dev/null || [ "$(md5sum ${TRADUCOESARQUIVO} 2>> /dev/null | cut -d ' ' -f 1)" != "${MD5SUMTRADUCOESARQUIVO}" ] || [ "$(md5sum index.html 2>> /dev/null | cut -d ' ' -f 1)" != "e9e215b6cb5ab1918bf5c8b5e0a85a9c" ] || [ "$(md5sum index_en.html 2>> /dev/null | cut -d ' ' -f 1)" != "fa789c053279708c77e03186b47ae2e9" ] || [ "$(md5sum index_cn.html 2>> /dev/null | cut -d ' ' -f 1)" != "1f761dd37d33158bea013b5e7da26649" ] || [ "$(md5sum index_es.html 2>> /dev/null | cut -d ' ' -f 1)" != "838f6fdbd4b3a0e933ef673ecebf774d" ] || [ "$(md5sum index_in.html 2>> /dev/null | cut -d ' ' -f 1)" != "b87085309903205c33c0befb65c97c3d" ] || [ "$(md5sum index_ar.html 2>> /dev/null | cut -d ' ' -f 1)" != "fbfe7576b7fbfa6d187b7fe6bbe49db3" ] || [ "$(md5sum index_ru.html 2>> /dev/null | cut -d ' ' -f 1)" != "b8a3c5f8124eea99f95300c13fa252e7" ] || [ "$(md5sum README.txt 2>> /dev/null | cut -d ' ' -f 1)" != "d30f59de0fc9c3dff6f9e44776cf28e4" ] || [ "$(md5sum README_en.txt 2>> /dev/null | cut -d ' ' -f 1)" != "59a77e9eae559f0a2e252e09f8f186dd" ] || [ "$(md5sum README_cn.txt 2>> /dev/null | cut -d ' ' -f 1)" != "508df74eb6a16227a323ace3c1cb3a78" ] || [ "$(md5sum README_es.txt 2>> /dev/null | cut -d ' ' -f 1)" != "4c8fdd5d406c462129b949bf9e6e32a7" ] || [ "$(md5sum README_in.txt 2>> /dev/null | cut -d ' ' -f 1)" != "364de8c4cc56e42c61f8a319a24e9469" ] || [ "$(md5sum README_ar.txt 2>> /dev/null | cut -d ' ' -f 1)" != "7fbc95a7d746edd2332604433a8248fc" ] || [ "$(md5sum README_ru.txt 2>> /dev/null | cut -d ' ' -f 1)" != "f64403989dbfc66b014321a8a308ac5e" ] || [ "$(md5sum README.md 2>> /dev/null | cut -d ' ' -f 1)" != "e0eb53a9832f77b1623defa6c1d7f4ef" ] || [ "$(md5sum antoniovandre_evalGUI.c 2>> /dev/null | cut -d ' ' -f 1)" != "e406505410622b093f7869ceb7b55ad0" ] || [ "$(md5sum antoniovandre_constantes.c 2>> /dev/null | cut -d ' ' -f 1)" != "38c989b6aeb167d53201107a0360a119" ] || [ "$(md5sum antoniovandre_eval_gui.pytemp 2>> /dev/null | cut -d ' ' -f 1)" != "10d7e0d5b616533c7c4b007f7803ddb7" ] || [ "$(md5sum antoniovandre_cores 2>> /dev/null | cut -d ' ' -f 1)" != "d4f1a5742145b4d790698eed8eaf7d50" ] || [ "$(md5sum antoniovandre_sobre.txt 2>> /dev/null | cut -d ' ' -f 1)" != "60186d7eb7ac439d06033f02f4194578" ] || [ "$(md5sum antoniovandre_sobre_en.txt 2>> /dev/null | cut -d ' ' -f 1)" != "73069bb38165ef4d079e03db7584f4b3" ] || [ "$(md5sum antoniovandre_sobre_cn.txt 2>> /dev/null | cut -d ' ' -f 1)" != "f37c1c337ff4ed0a97e22f277b4b5181" ] || [ "$(md5sum antoniovandre_sobre_es.txt 2>> /dev/null | cut -d ' ' -f 1)" != "dc063ee8483ba8b5631db6372524e79a" ] || [ "$(md5sum antoniovandre_sobre_in.txt 2>> /dev/null | cut -d ' ' -f 1)" != "204aca914d1f0c691d7df80fbdebacc2" ] || [ "$(md5sum antoniovandre_sobre_ar.txt 2>> /dev/null | cut -d ' ' -f 1)" != "dc94f47f33f76d2192919698d7dcf61c" ] || [ "$(md5sum antoniovandre_sobre_ru.txt 2>> /dev/null | cut -d ' ' -f 1)" != "a1af1a9ec52bd7f295ee2e701646111d" ] || [ "$(md5sum antoniovandre_precisao_real.txt 2>> /dev/null | cut -d ' ' -f 1)" != "4d095eeac8ed659b1ce69dcef32ed0dc" ] || [ "$(md5sum Math/antoniovandre_eval.c 2>> /dev/null | cut -d ' ' -f 1)" != "373f2f39445f1945b9a15a33da7f332f" ] || [ "$(md5sum antoniovandre_eval_gui_bgimage.png 2>> /dev/null | cut -d ' ' -f 1)" != "89cb7026063ee5e52291ede3d75d730d" ] || [ "$(md5sum antoniovandre_eval_gui_logo.png 2>> /dev/null | cut -d ' ' -f 1)" != "a7fcaccb1a993d65a644d781b4fa8d59" ] || [ "$(md5sum antoniovandre_eval_gui_logo_200p.png 2>> /dev/null | cut -d ' ' -f 1)" != "77d3ec93b5775b1029c6099ed6421b20" ] || [ "$(md5sum 'Bandeira do Brasil - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "386b8d48330e3a2d38f4b746499a54e4" ] || [ "$(md5sum 'Bandeira do Reino Unido - 510p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "d037587453fb413f1b8b0d7ac4f32861" ] || [ "$(md5sum 'Bandeira da China - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "367062c215b6050b1aa453e02cc44532" ] || [ "$(md5sum 'Bandeira da Espanha - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "b4acbd3849804d8799195ce621c96380" ] || [ "$(md5sum 'Bandeira da Índia - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "11543ddc6c7e1721aed4a3cf67c25267" ] || [ "$(md5sum 'Bandeira da Arábia Saudita - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "e460801eac7ce12c1eb996dab41ee4df" ] || [ "$(md5sum 'Bandeira da Rússia - 320p.png' 2>> /dev/null | cut -d ' ' -f 1)" != "58f050aef6773d8037164f2a28e3fb8b" ] || [ "$(md5sum DropBox-Icon.png 2>> /dev/null | cut -d ' ' -f 1)" != "96a57837a41cd9250459104b46d2f64e" ] || [ "$(md5sum 'GitHub logo.png' 2>> /dev/null | cut -d ' ' -f 1)" != "482dce8fe2ffc91fd287d8118f3488c2" ] || [ "$(md5sum "Mathematical Ramblings - Smartphone wallpaper 3 - Indexada.png" 2>> /dev/null | cut -d ' ' -f 1)" != "09e1440f8222651114f6f60d16889360" ] || [ "$(md5sum by-nc-sa.jpg 2>> /dev/null | cut -d ' ' -f 1)" != "e1746d3e902bc8e309335b983cd0bc00" ]; then echo -e "Diretório corrompido / Corrupted directory / 目錄損壞 / Directorio corrupto / Поврежденный каталог."; read; exit 1; fi; fi

if [ -n "${2}" ]; then ARGUMENTOLINGUISTICO="${2}"; else ARGUMENTOLINGUISTICO="nulo"; fi

case "${ARGUMENTOLINGUISTICO}" in
	"nulo")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
	;;
	"pt")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
	;;
	"br")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
	;;
	"en")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 2)")
	;;
	"us")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 2)")
	;;
	"cn")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 3)")
	;;
	"es")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 4)")
	;;
	"in")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 5)")
	;;
	"ar")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
	;;
	"ru")
		CARREGANDOCORES=$(eval echo "$(sed -n '1p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
	;;
	*)
		echo -e "Língua não reconhecida / Unrecognized langage / 無法識別的語言 / Lenguaje no reconocido / अपरिचित भाषा / لغة غير معروفة / Нераспознанный язык."; read; exit 1
	;;
esac

if ! [ -n "$AVCORES" ]; then echo -ne "${CARREGANDOCORES}"; if ! source "antoniovandre_cores" &>> /dev/null; then echo -e "Ocorreu um erro.\n"; else echo -e "${AV_VERDE}Ok${AV_SEMCOR}.\n"; fi; fi

