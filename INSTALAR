		INSTALARINTRODUCAO=$(eval echo "$(sed -n '12p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
		TRABALHANDOEM=$(eval echo "$(sed -n '13p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
