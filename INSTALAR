		TRABALHANDOESTATISTICAS=$(eval echo "$(sed -n '21p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		SUCESSODESINSTALAR=$(eval echo "$(sed -n '22p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		SUCESSOINSTALAR=$(eval echo "$(sed -n '23p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		ENTREEXPRESSAO=$(eval echo "$(sed -n '24p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		RESULTADO=$(eval echo "$(sed -n '25p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		ENTREEXPRESSAOCLIQUE=$(eval echo "$(sed -n '26p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		ARQUIVOSOBRE=$(eval echo "$(sed -n '27p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		AJUDA=$(eval echo "$(sed -n '28p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		CALCULAR=$(eval echo "$(sed -n '29p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		MAJUDA=$(eval echo "$(sed -n '30p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		SAIR=$(eval echo "$(sed -n '31p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
		DESINSTALARINTRODUCAO=$(eval echo "$(sed -n '32p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 6)")
	;;
	"ru")
		ENTRECOMOSARGUMENTOS=$(eval echo "$(sed -n '2p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		JAINSTALADO=$(eval echo "$(sed -n '3p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		CONFLITO=$(eval echo "$(sed -n '4p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		JADESINSTALADO=$(eval echo "$(sed -n '5p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		BASHNAOENCONTRADO=$(eval echo "$(sed -n '6p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		GCCNAOENCONTRADO=$(eval echo "$(sed -n '7p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		MD5SUMNAOENCONTRADO=$(eval echo "$(sed -n '8p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		PYTHONNAOENCONTRADO=$(eval echo "$(sed -n '9p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TCLTKNAOENCONTRADO=$(eval echo "$(sed -n '10p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		INSTALEDODIRETORIO=$(eval echo "$(sed -n '11p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		INSTALARINTRODUCAO=$(eval echo "$(sed -n '12p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOEM=$(eval echo "$(sed -n '13p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		SUCESSO=$(eval echo "$(sed -n '14p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		OCORREUFALHA=$(eval echo "$(sed -n '15p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOPYTHON=$(eval echo "$(sed -n '16p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOSOBRE=$(eval echo "$(sed -n '17p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOPRECISAOREAL=$(eval echo "$(sed -n '18p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOBGIMAGE=$(eval echo "$(sed -n '19p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOEXECUTAVEL=$(eval echo "$(sed -n '20p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		TRABALHANDOESTATISTICAS=$(eval echo "$(sed -n '21p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		SUCESSODESINSTALAR=$(eval echo "$(sed -n '22p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		SUCESSOINSTALAR=$(eval echo "$(sed -n '23p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		ENTREEXPRESSAO=$(eval echo "$(sed -n '24p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		RESULTADO=$(eval echo "$(sed -n '25p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		ENTREEXPRESSAOCLIQUE=$(eval echo "$(sed -n '26p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		ARQUIVOSOBRE=$(eval echo "$(sed -n '27p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		AJUDA=$(eval echo "$(sed -n '28p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		CALCULAR=$(eval echo "$(sed -n '29p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		MAJUDA=$(eval echo "$(sed -n '30p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		SAIR=$(eval echo "$(sed -n '31p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
		DESINSTALARINTRODUCAO=$(eval echo "$(sed -n '32p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 7)")
	;;
	*)
		echo -e "${AV_VERMELHO}Língua não reconhecida / Unrecognized langage / 無法識別的語言 / Lenguaje no reconocido / अपरिचित भाषा / لغة غير معروفة / Нераспознанный язык${AV_SEMCOR}."; exit 1
	;;
esac

if [ "${1}" != "desinstalar" ] && [ "${1}" != "instalar" ]; then echo -e "${AV_VERMELHO}${ENTRECOMOSARGUMENTOS}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ] && hash antoniovandre_eval_gui &>> /dev/null; then echo -e "${AV_VERMELHO}${JAINSTALADO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ] && hash antoniovandre_eval echo &>> /dev/null; then echo -e "${AV_VERMELHO}${CONFLITO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "desinstalar" ] && ! hash antoniovandre_eval_gui &>> /dev/null; then echo -e "${AV_VERMELHO}${JADESINSTALADO}${AV_SEMCOR}."; exit 1; fi

if ! hash bash &>> /dev/null; then echo -e "${AV_VERMELHO}${BASHNAOENCONTRADO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ] && ! hash gcc &>> /dev/null; then echo -e "${AV_VERMELHO}${GCCNAOENCONTRADO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ] && ! hash md5sum &>> /dev/null; then echo -e "${AV_VERMELHO}${MD5SUMNAOENCONTRADO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ] && ! hash python3 &>> /dev/null; then echo -e "${AV_VERMELHO}${PYTHONNAOENCONTRADO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ] && ! ls INSTALAR &>> /dev/null; then echo -e "${AV_VERMELHO}${INSTALEDODIRETORIO}${AV_SEMCOR}."; exit 1; fi

if [ "${1}" == "instalar" ]; then if wget -T $TIMEOUTMENSAGEM -qO- https://bit.ly/antoniovandreevalguimensagemdl 2>> /dev/null; then echo -e ""; fi; echo -e "${INSTALARINTRODUCAO}"; read; echo -e "#define ARQUIVO_MATH_ESTATISTICAS \"${HOME}/antoniovandre_math_estatisticas.txt\"\n" > antoniovandre_extra.c; else echo -e "${DESINSTALARINTRODUCAO}"; read; fi

DIR="${PWD}"
cd "${SOURCEDIR}"

OLDIFS="${IFS}"
IFS=$'\n'

for FILE in `ls -1 *.c`; do echo -ne "${TRABALHANDOEM} \"${FILE}\"... "; if [ "${1}" == "desinstalar" ]; then if rm "${DESTDIR}${FILE%%.*}" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; else if gcc "${FILE}" -o "${FILE%%.*}" -lm &>> /dev/null; then if mv "${FILE%%.*}" "${DESTDIR}" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; rm antoniovandre_extra.c &>> /dev/null; exit 1; fi; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; rm antoniovandre_extra.c &>> /dev/null; exit 1; fi; fi; done

IFS="${OLDIFS}"

cd .. &>> /dev/null

rm antoniovandre_extra.c &>> /dev/null

echo -n "${TRABALHANDOPYTHON} "; if [ "${1}" == "desinstalar" ]; then if rm "${DESTDIR}antoniovandre_eval_gui.py" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; else if cp antoniovandre_eval_gui.pytemp antoniovandre_eval_gui.pytempt && sed -i -e "s#HOME#${HOME}#g" antoniovandre_eval_gui.pytempt && sed -i -e "s/ENTREEXPRESSAOCLIQUE/${ENTREEXPRESSAOCLIQUE}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/RESULTADO/${RESULTADO}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/ENTREEXPRESSAO/${ENTREEXPRESSAO}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/MAJUDA/${MAJUDA}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/CALCULAR/${CALCULAR}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/AJUDA/${AJUDA}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/SAIR/${SAIR}/g" antoniovandre_eval_gui.pytempt && sed -i -e "s/TIMEOUTMENSAGEM/${TIMEOUTMENSAGEM}/g" antoniovandre_eval_gui.pytempt && mv antoniovandre_eval_gui.pytempt "${DESTDIR}antoniovandre_eval_gui.py" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; rm antoniovandre_eval_gui.pytempt &>> /dev/null; exit 1; fi; fi

echo -n "${TRABALHANDOSOBRE} "; if [ "${1}" == "desinstalar" ]; then if rm "${DESTDIRSHARE}antoniovandre_sobre.txt" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; else if cp "${ARQUIVOSOBRE}" "${DESTDIRSHARE}antoniovandre_sobre.txt" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; fi

echo -n "${TRABALHANDOPRECISAOREAL} "; if [ "${1}" == "desinstalar" ]; then if rm "${DESTDIRSHARE}antoniovandre_precisao_real.txt" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; else if cp antoniovandre_precisao_real.txt ${DESTDIRSHARE} &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; fi

echo -n "${TRABALHANDOBGIMAGE} "; if [ "${1}" == "desinstalar" ]; then if rm "${DESTDIRSHARE}antoniovandre_eval_gui_bgimage.png" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; else if cp antoniovandre_eval_gui_bgimage.png ${DESTDIRSHARE} &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; fi

echo -n "${TRABALHANDOEXECUTAVEL} "; if [ "${1}" == "desinstalar" ]; then if rm "${DESTDIR}antoniovandre_eval_gui" &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; exit 1; fi; else if echo -e "#!/bin/bash\nDIR=\$PWD\ncd \"${HOME}\"\npython3 ${DESTDIR}antoniovandre_eval_gui.py\ncd \"\${DIR}\"\n" > antoniovandre_eval_gui && chmod a+x antoniovandre_eval_gui &>> /dev/null && mv antoniovandre_eval_gui ${DESTDIR} &>> /dev/null; then echo -e "${AV_VERDE}${SUCESSO}${AV_SEMCOR}."; else echo -e "${AV_VERMELHO}${OCORREUFALHA}${AV_SEMCOR}."; rm antoniovandre_eval_gui &>> /dev/null; exit 1; fi; fi

if [ "${1}" == "desinstalar" ]; then echo -n "${TRABALHANDOESTATISTICAS} "; rm "${HOME}/antoniovandre_math_estatisticas.txt" &>> /dev/null; echo -e "${AV_VERDE}Ok${AV_SEMCOR}."; fi

if [ "${1}" == "desinstalar" ]; then echo -e "\n${SUCESSODESINSTALAR}."; else echo -e "\n${SUCESSOINSTALAR}."; fi

cd "${DIR}"
