		OCORREUFALHA=$(eval echo "$(sed -n '15p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
		TRABALHANDOPYTHON=$(eval echo "$(sed -n '16p' < ${TRADUCOESARQUIVO} | cut -d '%' -f 1)")
